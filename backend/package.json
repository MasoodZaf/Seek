{
  "name": "seek-backend",
  "version": "1.0.0",
  "description": "Professional backend API for Seek coding learning platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "seed:tutorials": "node scripts/seedAllTutorials.js",
    "seed:comprehensive": "node scripts/seedComprehensiveTutorials.js",
    "seed:full": "node scripts/seedFullTutorials.js",
    "seed:full:doubled": "node scripts/seedFullTutorialsDoubled.js",
    "seed:games": "node scripts/seedLearningGames.js",
    "seed:challenges": "node scripts/seedChallenges.js",
    "seed:challenges:expanded": "node scripts/seedExpandedChallenges.js",
    "seed:challenges:100": "node scripts/seed100Challenges.js",
    "seed:databases": "node scripts/seedDatabaseTutorials.js",
    "seed:databases:doubled": "node scripts/seedDatabaseTutorialsDoubled.js",
    "seed:typescript-java": "node scripts/seedTypeScriptJavaTutorials.js",
    "seed:all": "npm run seed:tutorials && npm run seed:games && npm run seed:challenges:100 && npm run seed:databases && npm run seed:typescript-java",
    "seed:all:doubled": "npm run seed:full:doubled && npm run seed:databases:doubled && npm run seed:games && npm run seed:challenges:100",
    "backup:db": "node scripts/backup-db.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie": "^0.7.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dockerode": "^4.0.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "isolated-vm": "^6.0.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^5.12.2",
    "sequelize": "^6.37.7",
    "socket.io": "^4.7.5",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tar-stream": "^3.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
